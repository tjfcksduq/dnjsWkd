				click : function(a) {
					a.stop();
					var d = a.element;
					if (d.tagName == "A") {
						for (var c = 0, f = this.aTab.length; c < f; c++) {
							var b = $Element(this.aTab[c]).parent();
							if (this.aTab[c] == d) {
								if (!b.hasClass("select")) {
									b.addClass("select");
								}
								$Element(this.aLayer[c]).show();
								nhn.common.ellipsis(this.aLayer[c]);
							} else {
								b.removeClass("select");
								$Element(this.aLayer[c]).hide();
							}
